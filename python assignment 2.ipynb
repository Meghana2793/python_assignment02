    "#31. Flatten a nested tuple.\n",
    "def flatten_tuple(t):\n",
    "    result = []\n",
    "    for i in t:\n",
    "        if isinstance(i, tuple):\n",
    "            result.extend(flatten_tuple(i))\n",
    "        else:\n",
    "            result.append(i)\n",
    "    return tuple(result)\n",
    "nested_tuple = (1, (2, 3), (4, (5, 6), 7))\n",
    "flat_tuple = flatten_tuple(nested_tuple)\n",
    "print(flat_tuple)  "
